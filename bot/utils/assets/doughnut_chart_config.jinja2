{
    type: 'doughnut',
    data: {labels: ['Online', 'Offline'], datasets: [{data: {{ data_string }}}]},
    options: {
        plugins: {
            doughnutlabel: {
                labels: [{text: {{ total_string }}, font: {size: 20}}, {text: 'total'}]
            },
            datalabels: {
                formatter: (value) => {
                    return new Date(value * 1000).toISOString().substr(11, 8);
                },
                backgroundColor: '#eee',
                borderRadius: 3
            }
        }
    }
}